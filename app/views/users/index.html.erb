<h1>Usuários</h1>

<%= link_to 'Novo Usuário', new_user_path %>

<ul>
  <% @users.each do |user| %>
    <li>
      <%= user.email %>
      | <%= link_to 'Editar', edit_user_path(user) %>
      | <%= link_to 'Remover', user_path(user), method: :delete, data: { confirm: "Tem certeza?" } %>
    </li>
  <% end %>
</ul>
